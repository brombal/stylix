{
  "name": "@stylix/core",
  "version": "5.1.0",
  "description": "React, with style. Add styles to your React apps with props: the easy, natural, and low learning curve approach.",
  "keywords": ["react", "styles", "css", "css-in-js", "emotion", "styled-components"],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run clean && rollup -c",
    "test": "jest --coverage",
    "clean": "rm -rf dist",
    "lint": "biome check --write ./src",
    "prepare": "husky",
    "update-css-props": "tsx scripts/updateCssProps.ts"
  },
  "author": "Alex Brombal",
  "license": "MIT",
  "homepage": "https://stylix.dev/",
  "bugs": "https://github.com/brombal/stylix/issues",
  "repository": "https://github.com/brombal/stylix",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "csstype": "^3.1.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mdn-data": "^2.14.0",
    "rollup": "^4.29.1",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
