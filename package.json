{
  "name": "@stylix/core",
  "version": "6.3.0",
  "description": "React, with style. Add styles to your React apps with props: the easy, natural, and low learning curve approach.",
  "keywords": [
    "react",
    "styles",
    "css",
    "css-in-js",
    "emotion",
    "styled-components"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run clean && rollup -c",
    "test": "jest --coverage",
    "clean": "rm -rf dist",
    "lint": "biome check --write ./src",
    "prepare": "husky",
    "update-css-props": "tsx scripts/updateCssProps.ts"
  },
  "author": "Alex Brombal",
  "license": "MIT",
  "homepage": "https://github.com/brombal/stylix",
  "bugs": "https://github.com/brombal/stylix/issues",
  "repository": "https://github.com/brombal/stylix",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "csstype": "^3.2.3",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "mdn-data": "^2.27.1",
    "rollup": "^4.59.0",
    "rollup-plugin-dts": "^6.3.0",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
